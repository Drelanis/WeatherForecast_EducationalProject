{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "common/*": ["src/common/*"],
      "context/*": ["src/context/*"],
      "apollo/*": ["apollo/*"],
      "assets/*": ["assets/*"],
      "components/*": ["src/components/*"],
      "hooks/*": ["src/hooks/*"],
      "screens/*": ["src/screens/*"],
      "lib/*": ["src/lib/*"]
    },
    "types": ["@types/styled-components-react-native"]
  }
}
